/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
        unordered_map <ListNode*, int> ump;
        ListNode* temp = head;
        
        while(temp != NULL){
            if(ump.find(temp) != ump.end())
                return temp;
            ump[temp]++;
            temp = temp -> next;
        }
        return NULL;
    }
};
